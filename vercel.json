{
  "version": 2,
  "buildCommand": "cd ClientMain/todoClient && npm install && npm run build",
  "installCommand": "cd server && npm install",
  "public": "ClientMain/todoClient/dist",
  "functions": {
    "server/index.js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server"
    },
    {
      "source": "/(.*)",
      "destination": "/server"
    }
  ]
}
